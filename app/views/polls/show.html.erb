<section>
  <div class="row">
    <div class="col-sm-8 col-sm-offset-2">
      <div class="poll_single">
        <div class="well">
          <h2>Meeting Details</h2>
          <p>Title: <%= @poll.title %></p>
          <p>Address: <%= @poll.address %></p>
          <p>City: <%= @poll.city %></p>
          <p>State: <%= @poll.state %></p>
          <p>Invitee: <%= @poll.invitee %></p>
  <!--     <%= @poll.invitee %> -->
        
     
      <p>Ready for another meeting? <%= link_to "New", new_poll_url, class: "btn btn-primary" %></p>
     
        <%= link_to "Home", polls_url, class: "btn btn-info" %>
        <% if @poll.user.id == current_user.id %>
          <%= link_to "Finalize Meeting", "/polls/#{@poll.id}/finalize", class: "btn btn-warning" %>
        <% end %>
        </div>
      </div>
    </div>
  </div>
</section>


